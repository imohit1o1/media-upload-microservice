{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "warn"
    },
    "env": {
      "node": true,
      "es6": true
    }
  }